<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script
      defer
      src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.7.2/axios.min.js"
      integrity="sha512-JSCFHhKDilTRRXe9ak/FJ28dcpOJxzQaCd3Xg8MyF6XFjODhy/YMCM8HW0TFDckNHWUewW+kfvhin43hKtJxAw=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    ></script>
    <link rel="stylesheet" href="../public/styles.css" />
    <title>Income</title>
  </head>
  <body>

    

    <div class="topnav">
      <a href="/">Home</a>
      <a href="/expenses">Expenses</a>
      <a class="active" href="/incomes">Income</a>
      <a href="/tracker">Tracker</a>
    </div>

    <h3>Incomes</h3>
    <section id="incomeSection" style="width: 400px; padding: 20px;">

      <% incomeList.forEach(incomeData => {%>
          <li><%= incomeData.date %></li>
          <p><%= incomeData.name %></p>
          <p>$<%= incomeData.amount %></p>
          <br>
          
    <%})%>

      <ul id="incomeList">
        
        <li>This is</li>
        <li>where</li>
        <li>Incomes will populate</li>
      </ul>
      <div id="status"></div>
    </section>

    
  </body>
</html>
